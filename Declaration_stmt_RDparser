D → T id L ;
T → int | float | char
L → , id L | ε


#include<stdio.h>
#include<stdlib.h>

char str[30];
int j=0;

int D(); int L(); int T();

int D(){
    if(T()==1){
        printf("Expanding T\n");
        if(str[j]=='i'){
            j++;
            printf("Consuming i\n");
            if(L()==1){
                printf("Expanding T\n");
                if(str[j]==';'){
                    j++;
                    printf("Consuming ;\n");
                    printf("D->TIL;\n");
                    return 1;
                }
                else return 0;
            }
            else return 0;
        }
        else return 0;
    }
    else return 0;
}

int T(){
    if(str[j]=='I'){
        j++;
        printf("Consuming I\n");
        printf("T->I\n");
        return 1;
    }
    else if(str[j]=='f'){
        j++;
        printf("Consuming f\n");
        printf("T->f\n");
        return 1;
    }
    else if(str[j]=='c'){
        j++;
        printf("Consuming c\n");
        printf("T->c\n");
        return 1;
    }
    else return 0;
}


int L(){
    if(str[j]==','){
        j++;
        printf("Consuming ,\n");
        if(str[j]=='i'){
            j++;
            printf("Consuming i\n");
            if(L()==1){
                printf("Expanding L\n");
                printf("L->,iL\n");
                return 1;
            }
            else return 0;
        }
        else return 0;
    }
    else return 1;
}

int main(){
    printf("Enter the sting\n");
    scanf("%s",str);
    if(D()==1&&str[j]=='\0'){
        printf("Accepted\n");
    }
    else printf("Rejected\n");
}
